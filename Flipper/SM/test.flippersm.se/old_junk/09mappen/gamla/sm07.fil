<SCRIPT LANGUAGE="JavaScript">

strTryck = "";
strStorlek = "";
intAnmalanpris = 190;   // anmälningsavgiften
intTrojpris = 130;      // tröjornas styckpris
intClassicspris = 100;  // classics anmälningsavgift
intBestallning = 0;     // tröjbeställningen so far


// räknar ut aktuella prisuppgifter
function update_price(frm)
   {
   return intAnmalanpris + intBestallning + Number(frm.chkClassics.checked)*intClassicspris;
   }


// kollar så att denna beställningsrad är fullständig och lägger isf till ny rad
function order_addline(frm)
   {
   ok = true;
   
   if(!strTryck && ok)
      {
      alert("Felaktigt tryck valt. Du måste välja vilket tryck du vill beställa.");
      frm.radTryck[0].focus();
      ok = false;
      }
   
   if((!isInteger(frm.txtAntal.value) || frm.txtAntal.value<0) && ok)
      {
      alert("Felaktigt antal tröjor valt. Du måste välja ett helt antal tröjor.");
      frm.txtAntal.focus();
      ok = false;
      }
   
   if(!strStorlek && ok)
      {
      alert("Ingen storlek vald. Du måste välja antingen S, M, L , XL eller XXL.");
      frm.radStorlek[0].focus();
      ok = false;
      }
   
   if(ok)
      {
      strOrder = strTryck + " ; " + frm.txtAntal.value + " st ; " + strStorlek + "\n";
      frm.txtOrder.value = frm.txtOrder.value + strOrder;
      intBestallning = intBestallning + Number(frm.txtAntal.value)*intTrojpris;
      frm.txtPris.value = update_price(frm) + " kr";
      
      order_clear(frm);
      }
   
   return true;
   }


// nollställer textrutan inför nästa beställningsrad
function order_clear(frm)
   {
   strTryck = "";
   for(var c=0; c<2; c++)
      {
      frm.radTryck[c].checked = false;
      }
   frm.txtAntal.value = "1";
   strStorlek = "";
   for(var c=0; c<5; c++)
      {
      frm.radStorlek[c].checked = false;
      }
   return true;
   }



// förladdar bilderna till tröjbytet
var imgSM = new Image;
imgSM.src = "http://www.callebe.com/flipper/sm07/tshirt.sm07.png";
var imgElvira = new Image;
imgElvira.src = "http://www.callebe.com/flipper/sm07/tshirt.elvira.png";

// byter tröjbild
function image_change(frm)
   {
   if(frm.radTryck[0].checked)  // SM
      {
      frm.imgShirt.src = imgSM.src;
      frm.imgLargeshirt.src = imgSM.src;
      }
   if(frm.radTryck[1].checked)  // Elvira
      {
      frm.imgShirt.src = imgElvira.src;
      frm.imgLargeshirt.src = imgElvira.src;
      }
   return true;
   }




function checkRegistration(frm)
   {
// kollar så att denna anmälan är fullständig
   ok = true;
   
   if((!frm.txtTag.value.length || isBlank(frm.txtTag.value)) && ok)
      {
      alert("Felaktig anmälan! Du måste ange din tag.");
      frm.txtTag.focus();
      ok = false;
      }
   
   if((!frm.txtFirstname.value.length || isBlank(frm.txtFirstname.value)) && ok)
      {
      alert("Felaktig anmälan! Du måste ange ditt förnamn.");
      frm.txtFirstname.focus();
      ok = false;
      }
   
   if((!frm.txtLastname.value.length || isBlank(frm.txtLastname.value)) && ok)
      {
      alert("Felaktig anmälan! Du måste ange ditt efternamn.");
      frm.txtLastname.focus();
      ok = false;
      }
   
   if((!frm.txtAddress.value.length || isBlank(frm.txtAddress.value)) && ok)
      {
      alert("Felaktig anmälan! Du måste ange din adress.");
      frm.txtAddress.focus();
      ok = false;
      }
   
   if((!frm.txtCity.value.length || isBlank(frm.txtCity.value)) && ok)
      {
      alert("Felaktig anmälan! Du måste ange din hemstad.");
      frm.txtCity.focus();
      ok = false;
      }
   
   if((frm.txtPhone.value.length<9 || isBlank(frm.txtPhone.value)) && ok)
      {
      alert("Felaktig anmälan! Du måste ange ditt telefonnummer inklusive riktnummer.");
      frm.txtPhone.focus();
      ok = false;
      }
   
   if((!frm.txtEmail.value.length || isBlank(frm.txtEmail.value) || frm.txtEmail.value.indexOf("@")==-1) && ok)
      {
      alert("Felaktig anmälan! Du måste ange din emailadress och den måste innehålla ett snabel-a.");
      frm.txtEmail.focus();
      ok = false;
      }
   
   if((frm.radTryck[0].checked || frm.radTryck[1].checked) && ok)
     {
     if(!confirm("Du har valt ett tröjtryck på en tröja som du ännu inte lagt till din beställning. Vill du ändå fortsätta med din anmälan?\n\nTryck på \"OK\" om du vill gå vidare.\n\nTryck på \"Cancel\" om du vill gå tillbaka och kontrollera i textrutan märkt \"Beställning:\" att du beställt de tröjor du vill."))
       {
       frm.txtOrder.focus();
       ok = false;
       }
     }
   
   if((frm.radStorlek[0].checked || frm.radStorlek[1].checked || frm.radStorlek[2].checked || frm.radStorlek[3].checked || frm.radStorlek[4].checked) && ok)
     {
     if(!confirm("Du har valt en tröjstorlek på en tröja som du ännu inte lagt till din beställning. Vill du ändå fortsätta med din anmälan?\n\nTryck på \"OK\" om du vill gå vidare.\n\nTryck på \"Cancel\" om du vill gå tillbaka och kontrollera i textrutan märkt \"Beställning:\" att du beställt de tröjor du vill."))
       {
       frm.txtOrder.focus();
       ok = false;
       }
     }
   
   
// all seems to be ok, go ahead with registration (spam protection is checked in next file)
   if(ok)
     {
     frm.submit();
     }
   
   return ok;
   }


function clearRegistration(frm)
   {
